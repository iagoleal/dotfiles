!!! urxvt configurations
URxvt.scrollBar: false

!Fonts
URxvt.font: xft:Source Code Pro:style=Semibold:size=11:antialias=true:hinting=false,xft:Source Han Sans:style=Semibold:size=20:antialias=true:hinting=false,xft:Material Design Icons:style=Regular:size=10
!URxvt.letterSpace: -1
!URxvt.font: xft:Hack:style=Regular:size=11:antialias=true:hinting=false

! Nice font options: Hack, Source Code Pro, Fantasque, Inconsolata


!! Xterm config
XTerm*termName: xterm-256color
XTerm*faceName: xft:Source Code Pro:style=Semibold:antialias=true:hinting=false
XTerm*faceSize: 11
XTerm*renderFont: true
XTerm*locale: true
! allows alt+key to be used for commands
XTerm*metaSendsEscape:  true


